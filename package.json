{
  "name": "token-launch",
  "version": "1.3.1",
  "description": "Token Launch - Connect your design to development automatically. Extract design tokens from Figma and push to GitHub with one click.",
  "author": "Silvia T. <silvtgit@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SilvT/Token-Launch-Plugin.git"
  },
  "keywords": [
    "figma",
    "design-tokens",
    "design-system",
    "github",
    "automation",
    "token-launch",
    "figma-plugin",
    "design-to-code"
  ],
  "dependencies": {
    "@create-figma-plugin/utilities": "^4.0.3",
    "octokit": "^5.0.3",
    "phosphor-icons": "^1.4.2",
    "phosphor-react": "^1.4.1",
    "simple-git": "^3.28.0",
    "task-master-ai": "^0.28.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.119",
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@figma/plugin-typings": "^1.109.0",
    "@types/node": "^24.5.2",
    "typescript": ">=5"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "build:dev": "build-figma-plugin --typecheck",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "token-launch",
    "name": "Token Launch",
    "main": "src/main.ts",
    "documentAccess": "current-page",
    "networkAccess": {
      "allowedDomains": [
        "https://api.github.com",
        "https://unpkg.com"
      ],
      "reasoning": "Required for GitHub integration and Phosphor Icons CDN - GitHub API for pushing design tokens and unpkg.com for icon fonts"
    }
  }
}
